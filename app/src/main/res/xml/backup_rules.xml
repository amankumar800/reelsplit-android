<?xml version="1.0" encoding="utf-8"?>
<!--
    Legacy backup rules for Android 11 and below (API < 31).
    Excludes cache from Auto Backup to prevent large video files
    from consuming the user's Drive backup quota.
-->
<full-backup-content>
    <include domain="database" path="." />
    <include domain="sharedpref" path="." />
    <exclude domain="cache" path="." />
    <exclude domain="external" path="." />
</full-backup-content>
